/* pages/rsslist.wxss */

.delete-box {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
}

.delete-bar {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 0;
  right: 0;
}

.delete {
  position: absolute;
  font-size: 28rpx;
  width: 34rpx;
  height: 34rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 50%;
  background-color: red;
  top: 0;
  right: 0;
}

.rss-more {
  font-size: 60rpx;
  padding-top: 48rpx;
  color: #3064A3;
}

.guidpic{
  width: 100%;
}